<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:fitsSystemWindows="true"
    tools:context=".karyawan.KaryawanActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="Data Nasabah"
        android:textAlignment="center"
        android:textColor="#00D1A5"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_transaksi_nasabah"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="192dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="43dp"
        tools:listitem="@layout/list_transaksi_nasabah" />


    <TextView
        android:id="@+id/tvNamaKaryawan"
        android:layout_width="364dp"
        android:layout_height="17dp"
        android:layout_margin="5dp"
        android:text="Nama Nasabah"
        app:layout_constraintBottom_toTopOf="@+id/tvEmail"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="25dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvEmail"
        android:layout_width="364dp"
        android:layout_height="17dp"
        android:layout_margin="5dp"
        android:text="Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="107dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvNoTelp"
        android:layout_width="364dp"
        android:layout_height="17dp"
        android:layout_margin="5dp"
        android:text="No Telp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="131dp" />

    <TextView
        android:id="@+id/tvSaldo"
        android:layout_width="364dp"
        android:layout_height="17dp"
        android:layout_margin="5dp"
        android:text="Saldo "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="158dp" />


    <!--<TextView-->
    <!--android:id="@+id/textView"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:fontFamily="@font/nunito_sans_bold"-->
    <!--android:text="Dashboard"-->
    <!--android:textColor="#00D1A5"-->
    <!--android:textAlignment="center"-->
    <!--android:textSize="25dp" />-->


</android.support.constraint.ConstraintLayout>

